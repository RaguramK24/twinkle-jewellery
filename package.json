{
  "name": "twinkle-jewellery",
  "version": "1.0.0",
  "description": "A complete MERN stack jewellery catalog application with JSON-based storage",
  "main": "server/server.js",
  "scripts": {
    "start": "node server/server.js",
    "dev": "npm run dev --prefix server",
    "install-server": "npm install --prefix server",
    "install-client": "npm install --prefix client",
    "install-all": "npm run install-server && npm run install-client",
    "build-client": "npm run build --prefix client"
  },
  "keywords": [
    "jewellery",
    "catalog",
    "mern",
    "express",
    "react",
    "nodejs"
  ],
  "author": "Raguram K",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/RaguramK24/twinkle-jewellery"
  },
  "homepage": "https://www.twinklesjewellery.in",
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.0.0"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "multer": "^1.4.5-lts.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  }
}